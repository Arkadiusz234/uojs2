{
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "rules": {
        "strict": 1,
        "no-underscore-dangle": 1,
        "no-unused-vars": 1,
        "curly": 1,
        "no-multi-spaces": 1,
        "key-spacing": 1,
        "no-return-assign": 1,
        "consistent-return": 1,
        "no-shadow": 1,
        "no-use-before-define": 1
    },
    "env": {
        "browser": true,
        "node": true
    }
}
